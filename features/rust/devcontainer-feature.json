{
    "id": "rust",
    "version": "1.0.2",
    "name": "Rust",
    "description": "Install Rust",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "none"
            ],
            "default": "none",
            "description": "Select or enter a Rust version to install"
        }
    },
    "containerEnv": {
        "CARGO_HOME": "/usr/local/cargo",
        "PATH": "${PATH}:${CARGO_HOME}/bin"
    },
    "capAdd": [
        "SYS_PTRACE"
    ],
    "securityOpt": [
        "seccomp=unconfined"
    ],
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer"
			]
		}
	}
}
