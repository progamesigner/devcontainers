{
    "id": "docker",
    "version": "1.1.1",
    "name": "Docker Client & Docker Compose",
    "description": "Install Docker Client & Docker Compose",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "none",
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter a docker client version to install"
        },
        "buildx": {
            "type": "string",
            "proposals": [
                "none",
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter a docker-buildx version to install"
        },
        "compose": {
            "type": "string",
            "proposals": [
                "none",
                "latest"
            ],
            "default": "latest",
            "description": "Select or enter a docker-compose version to install"
        }
    },
    "containerEnv": {
        "DOCKER_BUILDKIT": "1"
    }
}
