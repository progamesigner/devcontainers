{
    "id": "deno",
    "version": "1.0.0",
    "name": "Deno",
    "description": "Install Deno",
    "options": {
        "version": {
            "type": "string",
            "proposals": [
                "none"
            ],
            "default": "none",
            "description": "Select or enter a Deno version to install"
        }
    },
    "containerEnv": {
        "GOPATH": "/opt/go",
        "GOROOT": "/usr/local/go",
        "PATH": "${GOROOT}/bin:${GOPATH}/bin:${PATH}"
    },
    "capAdd": [
        "SYS_PTRACE"
    ],
    "securityOpt": [
        "seccomp=unconfined"
    ]
}
